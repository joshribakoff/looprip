{
  "name": "agent-pipeline",
  "version": "0.1.0",
  "description": "Agent Pipeline System - Build AI-powered workflows",
  "private": true,
  "type": "module",
  "bin": {
    "p": "p"
  },
  "scripts": {
    "build": "./scripts/build.sh",
    "test": "npm --prefix runtime test -- --run",
    "test:all": "./test.sh",
    "clean": "cd runtime && npm run clean",
    "lint": "(cd runtime && npm run lint:prompts && npm run lint)",
    "p": "./p",
    "install:cli": "bash ./scripts/install.sh",
    "runtime:build": "npm -w runtime run build",
    "runtime:dev": "npm -w runtime run dev",
    "runtime:test": "npm -w runtime test",
    "runtime:lint": "npm -w runtime run lint",
    "interactive": "npm -w runtime run interactive",
    "agent": "env-cmd -f ./.env npm --prefix runtime run agent",
    "run": "npm -w runtime run cli -- run",
    "validate": "npm -w runtime run cli -- validate",
    "prompt:create": "npm -w runtime run prompt:create"
  },
  "devDependencies": {
    "env-cmd": "^10.1.0"
  }
}
